{
    "name": "Inception",
    "version": "0.1.0",
    "description": "An extension that allows you to travel to the very first commit of any Github repository",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [{
        "matches": ["https://github.com/*/*"],
        "js": ["content-script.js"]
    }],
    "browser_action": {
        "default_title": "Inception",
        "default_popup": "popup.html",
        "default_icon": "icons/icon_48.png"
    },
    "icons": {
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "manifest_version": 2
}