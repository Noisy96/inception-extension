{
    "name": "Inception",
    "version": "1.0",
    "description": "An extension that allows you to travel to the very first commit of any Github repository",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["https://github.com/*/*"],
            "js": ["content-script.js"]
        }
    ],
    "browser_action": {
        "default_title": "Inception"
    },
    "manifest_version": 2
}